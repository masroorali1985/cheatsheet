git config --global user.name "Masroor Ali"
git config --global user.email "masroor.ali@xxx.de"


git init				# to initialize the git repo
git status				# to check the status of the repo
git add FILE.txt			# to stage the file before comit
git add .				# to stage all files before comit
git comit -m "comit message"		# to comit the file with message
git log				# to show history of comit
git checkout hash			# to jump to previous version of the file
git checkout master			# to jump to master branch
git branch				# to list the branches
git branch newname			# to create a new branch
git clone https://github.com/xyzrepo	# to clone the github repository	
